      <TextField fx:id="tfCalories" layoutX="562.0" layoutY="182.0" prefHeight="26.0" prefWidth="196.0" />
      <TextField fx:id="tfProteins" layoutX="562.0" layoutY="209.0" prefHeight="26.0" prefWidth="196.0" />
      <TextField fx:id="tfFats" layoutX="562.0" layoutY="240.0" prefHeight="26.0" prefWidth="196.0" />
      <TextField fx:id="tfSodium" layoutX="562.0" layoutY="269.0" prefHeight="26.0" prefWidth="196.0" />
      <Label layoutX="505.0" layoutY="186.0" prefHeight="18.0" prefWidth="58.0" text="Calories:" />
      <Label layoutX="505.0" layoutY="213.0" prefHeight="18.0" prefWidth="58.0" text="Proteins:" />
      <Label layoutX="505.0" layoutY="244.0" prefHeight="18.0" prefWidth="58.0" text="Fats:" />
      <Label layoutX="505.0" layoutY="273.0" prefHeight="18.0" prefWidth="58.0" text="Sodium:" />
