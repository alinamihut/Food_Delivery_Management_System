      <Button fx:id="btnCreateProduct" layoutX="185.0" layoutY="465.0" mnemonicParsing="false" prefHeight="38.0" prefWidth="140.0" text="Create new product"  onAction="#goToCompositeProductScene"/>
      <TableView fx:id="tableMenu" layoutX="19.0" layoutY="117.0" onMouseClicked="#handleClickTableView" prefHeight="339.0" prefWidth="462.0">
