<?import javafx.scene.text.*?>

<AnchorPane prefHeight="565.0" prefWidth="772.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="presentation.Administrator">
